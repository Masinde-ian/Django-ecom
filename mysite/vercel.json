{
    "builds": [
        {
            "src": "mysite/wsgi.py",
            "use": "@vercel/python",
            "config": { 
                "maxLambdaSize": "15mb",
                "runtime": "python3.9",
                "env": {
                    "PG_CONFIG": "C:\\Program Files\\PostgreSQL\\16\\bin\\pg_config pip install psycopg2-binary"
                }
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "mysite/wsgi.py"
        }
    ]
}
  