{
    "builds": [{
        "src": "mysite/wsgi.py",
        "use": "@vercel/python",
        "config": { 
        "maxLambdaSize": "15mb",
         "runtime": "python3.9",
         "PG_CONFIG": "C:/Program Files/PostgreSQL/16/bin/pg_config.exe" 
        }
    }],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "mysite/wsgi.py"
        }
    ]
}
  